tVideoTimeTracking(duration) {\n\t\tif (!this.o.tracking.logCall)\n\t\t\treturn;\n\n\t\tif (this.trackVideoTime && this.trackVideoTime.duration === duration)\n\t\t\treturn;\n\n\t\tthis.trackVideoTime = {\n\t\t\tduration: duration,\n\t\t\tisInitialized: true,\n\t\t\tvideo25: {\n\t\t\t\tlogAction: this.o.tracking.action.video25,\n\t\t\t\ttime: duration * 0.25\n\t\t\t},\n\t\t\tvideo50: {\n\t\t\t\tlogAction: this.o.tracking.action.video50,\n\t\t\t\ttime: duration * 0.5\n\t\t\t},\n\t\t\tvideo75: {\n\t\t\t\tlogAction: this.o.tracking.action.video75,\n\t\t\t\ttime: duration * 0.75\n\t\t\t},\n\t\t\tvideo100: {\n\t\t\t\tlogAction: this.o.tracking.action.video100,\n\t\t\t\ttime: duration\n\t\t\t}\n\t\t};\n\t}\n\n\t// TODO: add support of different youtube urls to MediaElement and then this method can be removed.\n\tfunction getVideoType(options) {\n\t\tif (canPlayHls()) {\n\t\t\tif (options.transcodedFile) {\n\t\t\t\toptions.file = options.transcodedFile;\n\t\t\t}\n\t\t}\n\n\t\tvar url = options.file;\n\n\t\tvar youtubeType = url.match(/(youtu\\.be)|(youtube\\.com)/);\n\t\tif (youtubeType) {\n\t\t\treturn \"video/youtube\";\n\t\t}\n\n\t\tvar youtubeProtocol = url.match(/(youtube\\:\\/\\/)/);\n\t\tif (youtubeProtocol) {\n\t\t\tvar key = url.substring(url.lastIndexOf(\"/\") + 1, url.length);\n\t\t\toptions.file = \"http://www.youtube.com/watch?v=\" + key;\n\t\t\treturn \"video/youtube\";\n\t\t}\n\n\t\tvar flashType = url.lastIndexOf(\".flv\") === url.length - 4;\n\t\tif (flashType) {\n\t\t\treturn \"video/flv\";\n\t\t}\n\n\t\tvar hlsType = url.lastIndexOf(\".m3u8\") === url.length - 5;\n\t\tif (hlsType) {\n\t\t\treturn \"application/x-mpegurl\";\n\t\t}\n\n\t\tvar dashType = url.lastIndexOf(\".mpd\") === url.length - 4;\n\t\tif (dashType) {\n\t\t\treturn \"application/dash+xml\";\n\t\t}\n\n\t\treturn \"video/mp4\";\n\t}\n\n\tfunction getVideoWrapper(playerId) {\n\t\tvar $player = $(\"#\" + playerId);\n\n\t\tif ($player.is(\"video\")) {\n\t\t\treturn $player;\n\t\t}\n\n\t\tvar $newVideoTag = $(\"<video>\").attr(\"id\", playerId);\n\t\tvar wrapperAttr = $player.get(0).attributes;\n\n\t\t$.each(wrapperAttr, function () {\n\t\t\t$newVideoTag.attr(this.name, this.value);\n\t\t});\n\n\t\t$player.replaceWith($newVideoTag);\n\n\t\treturn $newVideoTag;\n\t}\n\n\tfunction videoPlayer($wrapper, options) {\n\t\tvar self = this;\n\t\tthis.o = $.extend(true, {}, defaults, options);\n\n\t\tvar playOnInit = isMobileDevice() ? false : this.o.playOnInit;\n\n\t\tvar videoType = getVideoType(this.o);\n\n\t\tvar $player,\n\t\t\tplayerId = this.o.playerId;\n\n\t\tif (!playerId) {\n\t\t\tplayerId = pluginName + \"_\" + ++playersCount;\n\t\t\t$player = $(\"<video>\").attr(\"id\", playerId).appendTo($wrapper);\n\t\t} else {\n\t\t\t$player = getVideoWrapper(playerId);\n\t\t}\n\n\t\tif (videoType !== \"video/youtube\" && !this.o.disableCC && this.o.ccUrl && this.o.ccLang) {\n\t\t\t$player.append(\n\t\t\t\t$(\"<track>\").attr(\"kind\", \"subtitles\").attr(\"src\", this.o.ccUrl).attr(\"srclang\", this.o.ccLang)\n\t\t\t);\n\t\t}\n\n\t\t$player\n\t\t\t.attr(\"preload\", \"none\")\n\t\t\t.attr(\"style\", \"width:100%; height:100%;\")\n\t\t\t.attr(\"controls\", \"controls\")\n\t\t\t.attr(\"autoplay\", playOnInit);\n\n\t\tif (this.o.image) {\n\t\t\t$player.attr(\"poster\", this.o.image);\n\t\t}\n\n\t\tvar subscribeEvent = function (element, eventName, func) {\n\t\t\t$(element).on(eventName, function (e) {\n\t\t\t\tif ($.isFunction(func)) {\n\t\t\t\t\tfunc.call(self, e);\n\t\t\t\t}\n\t\t\t\t$wrapper.trigger({\n\t\t\t\t\ttype: \"ccsPlayer\",\n\t\t\t\t\tplayer: self,\n\t\t\t\t\toriginalEvent: e\n\t\t\t\t});\n\t\t\t});\n\t\t};\n\t\tvar settings = {\n\t\t\tvideoHeight: this.o.height,\n\t\t\tvideoWidth: this.o.width,\n\t\t\tenablePluginDebug: true,\n\t\t\ttype: videoType,\n\t\t\tcontrols: true,\n\t\t\tenableAutosize: false,\n\t\t\tpluginPath: this.o.pluginPath,\n\t\t\tflashScriptAccess: \"always\",\n\t\t\tfeatures: [\"playpause\", \"progress\", \"current\", \"duration\", \"tracks\", \"volume\", \"fullscreen\"],\n\t\t\talwaysShowControls: false,\n\t\t\tshowPosterWhenEnded: true,\n\t\t\ttoggleCaptionsButtonWhenOnlyOne: true,\n\t\t\tpauseOtherPlayers: true,\n\t\t\tloop: false,\n\t\t\thideVideoControlsOnLoad: true,\n\t\t\tstartLanguage: $wrapper.data(\"cc-lang\"),\n\t\t\tsuccess: function (media, obj, mejs) {\n\t\t\t\tsubscribeEvent(media, \"play\", onPlay);\n\t\t\t\tsubscribeEvent(media, \"pause\", onPause);\n\t\t\t\tsubscribeEvent(media, \"seeking\", onSeek);\n\t\t\t\tsubscribeEvent(media, \"canplay\", onReady);\n\t\t\t\tsubscribeEvent(media, \"ended\", onComplete);\n\n\t\t\t\tsubscribeEvent(mejs.container, \"enteredfullscreen exitedfullscreen\", onFullscreenChange);\n\n\t\t\t\tmedia.addEventListener(\"timeupdate\", function (e) {\n\t\t\t\t\t/* Fixed CCSDT-6672 */\n\t\t\t\t\tif (e.currentTarget.ended)\n\t\t\t\t\t\treturn;\n\n\t\t\t\t\tonTime.call(self, e);\n\t\t\t\t});\n\n\t\t\t\tif (!isMobileDevice()) {\n\t\t\t\t\tmedia.addEventListener(\"canplay\", function (e) {\n\t\t\t\t\t\tsetTimeout(function() {\n\t\t\t\t\t\t\tsubscribeEvent(media, \"volumechange\", onVolumeChange);\n\t\t\t\t\t\t}, 100);\n\t\t\t\t\t});\n\t\t\t\t}\n\n\t\t\t\tmejs.setSrc(self.o.file);\n\t\t\t\tself.src = self.o.file;\n\t\t\t\tself.getSrc = function () { // TODO: Consider about removing of getter or the 'src' field.\n\t\t\t\t\treturn self.o.file;\n\t\t\t\t};\n\n\t\t\t\tself.play = function () {\n\t\t\t\t\tmejs.play();\n\t\t\t\t};\n\t\t\t\tself.pause = function () {\n\t\t\t\t\tmejs.pause();\n\t\t\t\t};\n\t\t\t\tself.duration = function () {\n\t\t\t\t\treturn mejs.media.duration;\n\t\t\t\t};\n\t\t\t\tself.getCurrentPosition = function () {\n\t\t\t\t\treturn mejs.getCurrentTime();\n\t\t\t\t};\n\t\t\t\tself.setMute = function (state) {\n\t\t\t\t\tmejs.setMuted(state);\n\t\t\t\t};\n\t\t\t\tself.setVolume = function (value) {\n\t\t\t\t\tmejs.setVolume(value);\n\t\t\t\t};\n\t\t\t\tself.setPlayerSize = function (width, height) {\n\t\t\t\t\tmejs.setPlayerSize(width, height);\n\t\t\t\t};\n\n\t\t\t\tif (!self.o.showNativeButton) {\n\t\t\t\t\t$(self.o.mejsPlayBtn).css(\"display\", \"none\");\n\t\t\t\t}\n\n\t\t\t\tself.setVolume(self.o.volume * 0.01);\n\n\t\t\t\tif (self.o.mute) {\n\t\t\t\t\tself.setMute(true);\n\t\t\t\t}\n\n\t\t\t\tself.isInvokedByUser = !self.o.initByAutoplay;\n\n\t\t\t\t/*\n\t\t\t\tWe cannot setup autoplay video setting in Chrome Browser (both desktop and mobile)\n\t\t\t\taccording to new Chrome's autoplay policies (from april 2018):\n\t\t\t\thttps://developers.google.com/web/updates/2017/09/autoplay-policy-changes\n\t\t\t\t*/\n\t\t\t\tif (isMobileDevice() || !playOnInit) {\n\t\t\t\t\tself.pause();\n\t\t\t\t}\n\t\t\t\telse {\n\t\t\t\t\tself.play();\n\t\t\t\t}\n\t\t\t},\n\t\t\terror: function (e) {\n\t\t\t\tonError.call(self, e);\n\t\t\t}\n\t\t};\n\n\t\tif (!this.o.disableCC) {\n\t\t\tsettings.youtubeIframeVars = { cc_load_policy: 1 }\n\t\t}\n\n\t\tself.playerId = playerId;\n\t\tself.mejs = new MediaElementPlayer(\"#\" + playerId, settings);\n\t\t$wrapper.data(\"videoPlayer\", self);\n\n\t\tvar $iframeContainer = $wrapper.find(\".mejs__iframe-overlay\");\n\n\t\t$wrapper.add($iframeContainer)\n\t\t\t.one(\"click\", function (e) {\n\t\t\t\tself.isInvokedByUser = true;\n\t\t\t\tself.autoSoundChange = !$(e.target).closest(\".mejs__volume-button\").length;\n\n\t\t\t\t$.isFunction(self.setMute) ? self.setMute(false) : self.o.mute = false;\n\t\t\t});\n\t};\n\n\tfunction canPlayHls() {\n\t\treturn mejs.Features.hasMSE || mejs.Features.supportsNativeHLS;\n\t}\n\n\tfunction isMobileDevice() {\n\t\treturn ccs_util.isMobile();\n\t}\n\n\tfunction initAutoPlay(videos, settings) {\n\t\tif (isMobileDevice())\n\t\t\treturn;\n\n\t\tif (videos.length === 0)\n\t\t\treturn;\n\n\t\t$(videos).each(function () {\n\t\t\t$(this).data(\"video-settings\", settings);\n\t\t\t$.ccsVideoPlayer.playersArr.push($(this));\n\t\t});\n\n\t\trunVideosAutoPlay();\n\n\t\tvar timeoutId;\n\t\t$(window).on(\"scroll.ccsplayer resize.ccsplayer\", function () {\n\t\t\tclearTimeout(timeoutId);\n\n\t\t\ttimeoutId = setTimeout(function () {\n\t\t\t\trunVideosAutoPlay();\n\t\t\t}, 500);\n\t\t});\n\t}\n\n\tfunction runVideosAutoPlay() {\n\t\tvar videosInViewport = getAllVideosInViewPort($.ccsVideoPlayer.playersArr);\n\n\t\tif (videosInViewport.length === 0) {\n\t\t\tstopInlineVideos($.ccsVideoPlayer.playersArr);\n\t\t} else {\n\t\t\tstartMostRelevantVideo(videosInViewport);\n\t\t}\n\t}\n\n\tfunction startMostRelevantVideo(videos) {\n\t\tif ($.ccsVideoPlayer.isFullscreen())\n\t\t\treturn;\n\t\tvar $closestToCenter = getClosestVideoToCenter(videos);\n\n\t\tif ($closestToCenter == null)\n\t\t\treturn;\n\n\t\tvar player = $closestToCenter.ccsVideoPlayer(\"getPlayer\");\n\n\t\tif (player == null) {\n\t\t\tvar playerId = $closestToCenter.data(\"player\");\n\t\t\tif (playerId && !$(\"#\" + playerId).length) {\n\t\t\t\t$closestToCenter.append(\n\t\t\t\t\t$(\"<div class='ccs-cc-inline-embedded-video-wrapper'>\").append(\n\t\t\t\t\t\t$(\"<video>\").attr(\"id\", $closestToCenter.data(\"player\"))));\n\t\t\t}\n\t\t\tvar settings = getVideoSettings($closestToCenter, $closestToCenter.data(\"video-settings\"));\n\t\t\t$closestToCenter.ccsVideoPlayer(settings);\n\t\t\treturn;\n\t\t}\n\n\t\tif (player.isInvokedByUser) {\n\t\t\treturn;\n\t\t}\n\n\t\tif (player.isCompleted) {\n\t\t\tvar index = $.ccsVideoPlayer.playersArr.indexOf($closestToCenter);\n\t\t\t$.ccsVideoPlayer.playersArr.splice(index, 1);\n\t\t\trunVideosAutoPlay();\n\t\t\treturn;\n\t\t}\n\n\t\tplayer.play();\n\t}\n\n\tfunction getAllVideosInViewPort(allVideos) {\n\t\tvar result = [];\n\n\t\tfor (var i = 0; i < allVideos.length; i++) {\n\t\t\tvar $video = allVideos[i];\n\t\t\tif (isElementInViewport($video)) {\n\t\t\t\tresult.push($video);\n\t\t\t}\n\t\t}\n\n\t\treturn result;\n\t}\n\n\tfunction getClosestVideoToCenter(videos) {\n\t\tif (videos.length === 0)\n\t\t\treturn null;\n\n\t\tvar $candidate = videos[0];\n\n\t\tfor (var i = 1; i < videos.length; i++) {\n\t\t\tvar candidateDistance = getDistanceToCenter($candidate);\n\t\t\tvar currentDistance = getDistanceToCenter(videos[i]);\n\n\t\t\tif (currentDistance < candidateDistance) {\n\t\t\t\t$candidate = videos[i];\n\t\t\t}\n\t\t}\n\n\t\treturn $candidate;\n\t}\n\n\tfunction getDistanceToCenter($video) {\n\t\tvar screenCenter = $(window).scrollTop() + $(window).height() / 2;\n\t\tvar videoCenter = $video.offset().top + $video.height() / 2;\n\n\t\treturn Math.abs(screenCenter - videoCenter);\n\t}\n\n\tfunction stopPlayer($playerWrapper) {\n\t\tvar player = $playerWrapper.ccsVideoPlayer(\"getPlayer\");\n\t\tif (player && $.isFunction(player.pause)) {\n\t\t\tplayer.pause();\n\t\t}\n\t}\n\n\tfunction stopInlineVideos(videos) {\n\t\t$.each(videos, function (index, $video) {\n\t\t\tvar player = $video.ccsVideoPlayer(\"getPlayer\");\n\t\t\tif (player && $.isFunction(player.pause) && !player.mejs.isFullScreen) {\n\t\t\t\tplayer.pause();\n\t\t\t}\n\t\t});\n\t}\n\n\tfunction isElementInViewport(element, spread) {\n\n\t\tspread = spread || 0;\n\n\t\tvar $element = $(element);\n\n\t\tvar windowsHeight = $(window).height();\n\t\tvar viewportScrollTop = $(window).scrollTop();\n\n\t\tvar areaTop = viewportScrollTop < spread ? 0 : viewportScrollTop - spread;\n\t\tvar areaBottom = viewportScrollTop + windowsHeight + spread;\n\n\t\tvar elementTop = $element.offset().top;\n\t\tvar elementBottom = elementTop + $element.height();\n\n\t\treturn elementBottom <= areaBottom\n\t\t\t\t&& elementTop >= areaTop\n\t\t\t\t&& $element.is(\":visible\");\n\t}\n\n\tfunction getVideoSettings($wrapper, settings) {\n\t\tvar defaults = {\n\t\t\tplayerId: $wrapper.data(\"player\"),\n\t\t\tfile: $wrapper.data(\"video-url\"),\n\t\t\ttranscodedFile: $wrapper.data(\"transcoded-video-url\"),\n\t\t\timage: $wrapper.data(\"image-url\"),\n\t\t\tccUrl: $wrapper.data(\"cc-url\"),\n\t\t\tccLang: $wrapper.data(\"cc-lang\"),\n\t\t\tccForceOnYouTube: $wrapper.data(\"cc-force-yt\"),\n\t\t\theight: $wrapper.data(\"media-height\") || \"100%\",\n\t\t\twidth: $wrapper.data(\"media-width\") || \"100%\",\n\t\t\tplayOnInit: true, // TODO: Consider about renaming of 'autoplay' to autoStart or playOnInit.\n\t\t\tinitByAutoplay: false,\n\t\t\tmute: true,\n\t\t\tvolume: 80,\n\t\t\tdebug: true,\n\t\t\ttracking: {\n\t\t\t\tlogCall: $wrapper.attr(\"data-video-log-call\"),\n\t\t\t\terrorLogCall: $wrapper.attr(\"data-error-log-call\")\n\t\t\t}\n\t\t};\n\n\t\tif (settings) {\n\t\t\tif (settings instanceof Function)\n\t\t\t\tsettings = settings($wrapper);\n\n\t\t\treturn $.extend(true, defaults, settings);\n\t\t}\n\t\treturn defaults;\n\t}\n\n\t$.fn[pluginName] = function (methodOrOptions, args) {\n\n\t\tif (this.length === 0)\n\t\t\treturn null;\n\n\t\tvar $element = this.eq(0);\n\t\tvar player;\n\t\tif (typeof methodOrOptions === \"object\") {\n\t\t\tplayer = new videoPlayer($element, methodOrOptions);\n\t\t\t$element.data(\"plugin_\" + pluginName, player);\n\n\t\t\treturn player;\n\t\t}\n\t\telse if (typeof methodOrOptions === \"undefined\") {\n\t\t\treturn $element.data(\"plugin_\" + pluginName);\n\t\t}\n\t\telse if (typeof methodOrOptions === \"string\") {\n\t\t\tplayer = $element.data(\"plugin_\" + pluginName);\n\t\t\tif (!player)\n\t\t\t\treturn null;\n\n\t\t\treturn player[methodOrOptions].call(player, args);\n\t\t}\n\t};\n\n\t$[pluginName] = {\n\t\tstopAllVideos: function () {\n\t\t\tfor (var playerIndex in mejs.players) {\n\t\t\t\tvar player = mejs.players[playerIndex];\n\t\t\t\tif (!player.paused && !player.ended)\n\t\t\t\t\tplayer.pause();\n\t\t\t}\n\t\t},\n\t\tisFullscreen: function () {\n\t\t\tif (mejs.Features.isIE)\n\t\t\t\treturn $(\"html\").hasClass(\"mejs-fullscreen\");\n\n\t\t\treturn mejs.Features.isFullScreen\n\t\t\t\t\t&& typeof mejs.Features.isFullScreen === \"function\"\n\t\t\t\t\t&& mejs.Features.isFullScreen();\n\t\t},\n\t\tcanPlayHls: canPlayHls,\n\t\tinitAutoPlay: initAutoPlay,\n\t\tstopInlineVideos: stopInlineVideos,\n\t\tstopPlayer: stopPlayer,\n\t\tgetVideoSettings: getVideoSettings,\n\t\tplayersArr: []\n